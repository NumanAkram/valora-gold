{
  "name": "mama-organic-website",
  "version": "1.0.0",
  "description": "Mama Organic - Natural Beauty Products Website",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@headlessui/react": "^1.7.14",
    "@heroicons/react": "^2.0.18",
    "axios": "^1.5.0",
    "chart.js": "^4.5.1",
    "clsx": "^2.0.0",
    "lucide-react": "^0.284.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0",
    "react-scripts": "^5.0.1",
    "react-transition-group": "^4.4.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.2",
    "html-webpack-plugin": "^5.6.5",
    "postcss": "^8.4.21",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.2.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "react-scripts start",
    "server": "cd server && npm start",
    "server:dev": "cd server && npm run dev",
    "server:seed": "cd server && npm run seed",
    "server:reset-users": "cd server && npm run reset-users",
    "dev:all": "concurrently \"npm run dev\" \"npm run server:dev\"",
    "install:all": "npm install && cd server && npm install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
